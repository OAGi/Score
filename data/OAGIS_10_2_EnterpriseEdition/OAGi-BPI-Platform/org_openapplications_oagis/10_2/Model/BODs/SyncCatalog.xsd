                        <xsd:element ref="Sync" id="oagis-id-e09dea6552274f71ac59cef29ee9272a"/>
                        <xsd:element ref="Catalog" maxOccurs="unbounded" id="oagis-id-97cbf77b35ac4451ba56f85a3bcc8710"/>
                </xsd:sequence>
        </xsd:complexType>
</xsd:schema>
