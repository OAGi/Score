		<xsd:sequence>
			<xsd:element ref="Change" id="oagis-id-40c3f1aa53ce4af7b425cdaaefbc52f9"/>
			<xsd:element ref="MergeWIP" maxOccurs="unbounded" id="oagis-id-d15ae93eda3d4ecdb1cfb770c47d8e40"/>
		</xsd:sequence>
	</xsd:complexType>
