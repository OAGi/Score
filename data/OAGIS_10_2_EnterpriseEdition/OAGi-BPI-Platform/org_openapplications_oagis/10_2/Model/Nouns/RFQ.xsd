	</xsd:complexType>
	<xsd:complexType name="RFQHeaderBaseType" id="oagis-id-775c7798eae24224a031d5ed78761565">
		<xsd:complexContent>
			<xsd:extension base="ProcurementHeaderBaseType">
				<xsd:sequence>
					<xsd:element ref="BuyerParty" id="oagis-id-dad39e11da3e4a6eafc4d2a14f8fee22" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The entity that is responsible for Buying the document or element in which it is associacted. Additionally, the contact  may be a person or a group or department.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RequesterParty" id="oagis-id-03d597fc4b5b444abdc71019db140b71" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The party that requested the associated entity.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PriorityCode" id="oagis-id-6f4c1547727f45598fb09739748431d0" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the priority or urgency of the associated entity. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EffectiveTimePeriod" id="oagis-id-0afd805b46ed4b8488f93d44b73e83db" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the time period in which the associated component is effective.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SupplierSelectionTimePeriod" id="oagis-id-0319af57040441ac989aaa101b38d02c" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the timeperiod that Supplier(s) will be selected</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="BiddingTimePeriod" id="oagis-id-80cf91d1fd814d3387bfe96f2f74c144" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the period in which bidding can occur.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="QualificationTerm" id="oagis-id-59529e34a87845e4bdea62052ea15b55" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is used to describe preferences that will be used to evaluate responses to an RFQ. For example, to identify a preference for sharing CAD data in VRML format the ID = VRML, Type = CAD and Value = 1 or Yes</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Message" id="oagis-id-d488083330794515ad3f10a6effaafa8" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
