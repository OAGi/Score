	<xsd:complexType name="TestResultsHeaderBaseType" id="oagis-id-f0cc78da3b7a4f09862af2b98c8ca48b">
		<xsd:annotation>
			<xsd:documentation>Test Results header communicates the status and the related parties interested in the raw test results</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="StatusHeaderBaseType">
				<xsd:sequence>
					<xsd:element ref="PriorityCode" id="oagis-id-a128cda3b4594bb1a53793fd5f3dbc17" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the priority or urgency of the associated entity. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="InspectionOrderID" id="oagis-id-d61b35247d5d4fc2905b61eb6ac33f02" minOccurs="0"/>
					<xsd:element ref="LotBatchID" id="oagis-id-0f95bcd3e2d547be945aaf37d5ed77b4" minOccurs="0"/>
					<xsd:element ref="ManufactureDateTime" id="oagis-id-c5c1607cf74d40008890513d65f4cc2c" minOccurs="0"/>
