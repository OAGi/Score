					<xsd:documentation source="http://www.openapplications.org/oagis/10">
						<ccts_Name>US, Center for Disease Control (CDC)</ccts_Name>
						<ccts_Definition>A division of the US Department of Health and Human Services. CDC promotes health and quality of life by preventing and controlling disease, injury, and disability.</ccts_Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="338">
