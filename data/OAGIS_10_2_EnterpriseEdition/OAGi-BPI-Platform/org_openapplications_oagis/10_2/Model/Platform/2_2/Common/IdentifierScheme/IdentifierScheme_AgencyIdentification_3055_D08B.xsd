					<xsd:documentation source="http://www.openapplications.org/oagis/10">
						<ccts_Name>US, Bureau of Labor Statistics (BLS)</ccts_Name>
						<ccts_Definition>A division of the US Department of Labor. BLS is the principal fact-finding agency for the Federal Government in the broad field of labor economics and statistics.</ccts_Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="335">
