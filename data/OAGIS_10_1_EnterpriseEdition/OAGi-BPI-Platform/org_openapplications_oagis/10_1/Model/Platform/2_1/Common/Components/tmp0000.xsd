	<xsd:complexType name="ScheduleExceptionBaseType" abstract="false" id="oagis-id-995620cad55b4e52878b653c3a765211" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Schedule Exception Type for detailing variants of a Schedule</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
