	<xsd:complexType name="CRMHeaderBaseType" abstract="false" id="oagis-id-e7a2d2cac3f140a1b2fa3e6c35fd535b" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">CRM Header Type for detailing the generic information for a CRM Noun</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="StatusHeaderBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="Name" id="oagis-id-99e3734d81804127875874db1505f04e" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the Name of the object in which associated.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CloseReasonCode" id="oagis-id-2f2bbc0491d643b68437e5c168cc5cdf" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Reason code for why the lead/opportunity was closed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Interests" id="oagis-id-f6f1f6de72524ce681d31d90ba764fcf" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Type of transaction the person or entity is interest in Ex. in case of Automobile, Buy. Sell. Lease or Trade, in some other business New, Renewal. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="TotalAmount" id="oagis-id-30c573e82fb94a15a9acad2fdcdaa1a9" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the total amount of the object associated. This includes the Price and the total of all the charges, allowances and taxes.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="BudgetedAmount" id="oagis-id-34d755aa4df24f3b983cb3294ef1ad6c" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The budgeted amount of performing this activity. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ReceivedCode" id="oagis-id-5fc066ad8fcd4ac4ab4a718c61bf08f6" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Method in which the lead was received.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SourceID" id="oagis-id-296926e4e8f5499e979a584c724420a9" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the source that brought the associated entity.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CustomerParty" id="oagis-id-c2ac05b7018748a98fa1bfbda60ee1f7" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the organization that is the end customer of the goods or services being provided in the associated transaction.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OwnerParty" id="oagis-id-8e8b734040f143579066c1ed72111b14" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is used to identify who or which organization owns the item or part. The content of this field is user defined based on a specific Customer or Supplier.

Synonyms are Vendor Consignment ID, Supplier Consignment ID</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SalesContact" id="oagis-id-dabff2bb93984c04b8057b4b5f3ce7d9" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the sales contact and sales department/organization that is responsible for the sale that the instance is associated.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CompetitorParty" id="oagis-id-cf173577ccbe467982abcc3cc0235ed4" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the organizations that are competing for this business.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MarketingProgram" id="oagis-id-6eee6313eae04ae7b927f815a3a678e3" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the Marketing Campaign that the Lead is eligible for</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="QuoteReference" id="oagis-id-d86741f56d0c45ae804003bda9c10ef9" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is a reference to a Quote</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SalesActivity" id="oagis-id-60c61bac5e6a44dcb295bcbf1316574b" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">An associated sales activity as a result of the lead/opportunity.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SalesTask" id="oagis-id-b872340f8daf4c7d8df4ed9a6df05f68" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This will tell all the associated Tasks which have been associated with this Opportunity. It should tell who created it, who owns it, what the status is, task Priority, Planned Start and End Date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CRMLineBaseType" abstract="false" id="oagis-id-74c34026553c4426b9a97c17ca325512" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">CRM Line Type for detailing the specific information contained in a CRM Noun</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="StatusLineBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="Item" id="oagis-id-d8c6246130bf48c48cfc9291c4bd26a4" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the Item associated with the Line or Detail of a transaction. The item provides the details of the generic Item component information. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Amount" id="oagis-id-2a714c68108d4423bdb9d0a79dcd5ced" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The monetary value of the object in which the amount is associated</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Quantity" id="oagis-id-7220afaf5a464dcf8877ddfe416437c7" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the quantity of the associated service or item that is addresses by the component in which the quantity is associated.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CompetitorParty" id="oagis-id-f50eb7c9aaa04652a0df9049f4882595" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the organizations that are competing for this business.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Party" id="oagis-id-48ef1dc9b9014e70b5ea7d884819a433" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="CompetingItem" id="oagis-id-72f2af50ed404c209cd56182e2546fd5" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Products of the CompetitorParty that compete on the specific Opportunity Line</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MarketingProgram" id="oagis-id-236dccf00d4048f884ab116d6c870957" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the Marketing Campaign that the Lead is eligible for</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SalesActivityType" abstract="false" id="oagis-id-b2a4eccb620d4f09aee7d8a3bb5cdb73" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Sales Activity Type for detailing a sales activity event or occurrence</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="SalesActivityBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="SalesActivityExtensionType" id="oagis-id-9b296006607d4dff9a8a27456e2d919a" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SalesActivityBaseType" abstract="false" id="oagis-id-41b670ee330c430e90e3e6b8beb07aa8" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Sales Activity Base Type for detailing the minimum information needed for a sales event or activity</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="DateTime" id="oagis-id-fe5f1a5d9fc4458785f142d312902b8a" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.starstandards.org">Date and time that the associated entity took place .</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MaterialRequested" id="oagis-id-9f4ef9340c9e47e09b740902847897ba" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.starstandards.org">Name of the materials the customer requested.  (i.e., brochure, materials, etc.)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MaterialSent" id="oagis-id-084d5191194b4323864b197c61a7e2ed" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.starstandards.org">Name of the materials sent to the customer. (i.e., brochure, materials, etc.)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EmailResultCode" id="oagis-id-565fbfc044e849018573c4a0dc52f4ce" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.starstandards.org">Indicates Whether an Email was Sent. Example could be: Sent, digit- - number of times sent, Received, Not Sent, Undeliverable, Email address not available</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RequestContactIndicator" id="oagis-id-b1d81bde22be4d70a4d73aa30c04b6a5" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates whether customer would like to be contacted.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Result" id="oagis-id-9f785e9ba43646c097e50175ab901345" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.starstandards.org">Result of Activity - Ex: Bought competing Vehicle </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="FreeFormTextGroup" id="oagis-id-50ecbaa18c21406d80d9709c6cc98f46" minOccurs="1" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="SalesActivity" type="SalesActivityType" abstract="false" nillable="false" id="oagis-id-0cca016b5afb4ff08b1e3e6558f6c21e">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">An associated sales activity as a result of the lead/opportunity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SalesTask" type="SalesTaskType" abstract="false" nillable="false" id="oagis-id-8dd064cfd61f4bfbafb8803fec776957">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Specifies details of a task within the sales cycle such as who created it, who owns it, what the status is, task Priority, Planned Start and End Date</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="SalesTaskBaseType" abstract="false" id="oagis-id-72f628ceb37349c9a263b3f1deb44f41" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Sales Task Type for detailing a task within the sales cycle</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:element ref="Subject" id="oagis-id-0b38b60d486f43fa84b09e834049043e" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A short subject which describes the task.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PriorityCode" id="oagis-id-90edf0ec8386435b85b553934bea184f" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the priority or urgency of the associated entity. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="FreeFormTextGroup" id="oagis-id-d69c017efefa44fd92860f15cbe821dd"/>
					<xsd:element ref="Status" id="oagis-id-b63c7172516b461690007ed8412785af" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the status of the associated object by providing the Status Code along with a description and when the status is effective.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CategoryCodes" id="oagis-id-48d305c595a2447face0b59cb5554f83" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Provides a list of category codes that identify the capabilities and features of the associated object.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PrivateIndicator" id="oagis-id-d70aa2afec4d4dc3a1fd820515296b74" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Whether the task is Private or Not.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ParentTaskID" id="oagis-id-a71cbf85933142a9a7b36262f899b1dd" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Ability to define the Task dependencies</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EscalationLevelCode" id="oagis-id-35308b43048e4e5a82edeba17269cd7b" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">In Customer support scenarios one can associate Escalation level which could be numbers or textual.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PlannedTimePeriod" id="oagis-id-234a817ae7de4d909c3301f4defe0b83" minOccurs="0"/>
					<xsd:element ref="ScheduledTimePeriod" id="oagis-id-e409ebccc48a4fa38be4f2294d834322" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the TimePeriod in which the work is Scheduled to be performed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ActualTimePeriod" id="oagis-id-6446c1bacf5941508fcdcc9a85e7dab9" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the time period in which the actual event(s) occurred.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OwnerParty" id="oagis-id-216c2d54ea0a4064babae6fc1589097c" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is used to identify who or which organization owns the item or part. The content of this field is user defined based on a specific Customer or Supplier.

Synonyms are Vendor Consignment ID, Supplier Consignment ID</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="AssignedToContact" id="oagis-id-a8d380919d3f4fc5bc47f171e58e5a2c" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">To who all the task is assigned to</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="AssigneeTypeCode" id="oagis-id-63a8107cf8824d37b95c0415250354e9" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Type of Assignee such as Employee, Customer Contact, Party, Party Relationship</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="AssigneeStatus" id="oagis-id-52fee87b3e0542b88f4fcc33e55ca9db" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Status of the Assignee, "Accepted", "Declined", </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SourceID" id="oagis-id-2986101f9c6342fcb56a1c9e3cd21fc1" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the source that brought the associated entity.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CustomerParty" id="oagis-id-893dda9b3e8b4d1ca4dcda07d99d1e68" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the organization that is the end customer of the goods or services being provided in the associated transaction.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SalesLeadReference" id="oagis-id-a921519188474993baf23d6fc42ccc0a" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Reference to an existing SalesLead that is associated with the given document.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OpportunityReference" id="oagis-id-f19ac91415aa4c8295d1cf6341f96411" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ServiceRequestReference" id="oagis-id-c4b00abb86104b738edc78a158a4666a" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="CompletionPercent" id="oagis-id-df8158619c9e441db657575cf6f05d01" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">How much of the task has been completed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CompletedDuration" id="oagis-id-f6e1684875854475813abb0cafdbe0da" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Time currently spent on the Task while it is in progress,</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PlannedEffortDuration" id="oagis-id-88ff281e341a4202bcbc0e2e3e202ea6" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Planned Effort could be in terms of weeks, days, man months, etc,</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ActualEffortDuration" id="oagis-id-6920ddd637a24c47be02cfcf76389caf" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Actual Effort could be in terms of weeks, days, man months, etc,</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RepeatingFrequencyCode" id="oagis-id-f72ee79c22be4285a2ece9bb8921b248" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">How often the task repeats ex, Hourly, weekly or Monthly</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Contact" id="oagis-id-4567349d6f1b4a6fb0e67964cf4cc54c" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Contacts are the point of communication with the given party. In the case of an organization it may be the requisitioner, the buyer, customer service, etc. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="ReferencesGroup" id="oagis-id-63c3776568da4fb080f197353eeb2256"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SalesTaskType" abstract="false" id="oagis-id-8c8fdb159d5f4e21a75c9c54eb79da1c" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Sales Task Type for detailing a task within the sales cycle</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="SalesTaskBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="SalesTaskExtensionType" id="oagis-id-3a5fcf1b52c54536a44c2cb0fcf9e58d" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AppointmentBaseType" abstract="false" id="oagis-id-1b30dd2bf6754955948665e91fcaf871" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Appointment Base Type for detailing the minimum information for referencing a specific event or action that needs to occur in a specific time and place</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="DateTime" id="oagis-id-ecf28e4d6b154adba0dbbb3f2a7db431" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.starstandards.org">Date and time that the associated entity took place .</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Location" id="oagis-id-e7d9a4ec271a40f0b96ef950c08cd5d5" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The location of a thing, as captured by an address (or addresses), GPS Coordinates, and/or in relation to other locations.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="FreeFormTextGroup" id="oagis-id-3e9c98218a5c452b8fa71399fd8a7093" minOccurs="1" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AppointmentType" abstract="false" id="oagis-id-e81adc8e50bd44aca49b3d8b98579f55" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Appointment Type for adding additional or application specific information to an appointment</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AppointmentBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="AppointmentExtensionType" id="oagis-id-dcf7ee90308b40caa084747cbcf8ab41" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="Appointment" type="AppointmentType" abstract="false" nillable="false" id="oagis-id-5161b69fc7f74756aff1a84ee29a2335">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies details of a specific occurrence of and event or action that occurs within a specific time and or place</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!---->
	<xsd:element name="Financing" type="FinancingType" abstract="false" nillable="false" id="oagis-id-1b17e2c9e3e24fbd82036eafba1ba22e">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies the details of a specific occurrence of an offer to provide funding</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="FinancingBaseType" abstract="false" id="oagis-id-3a9820cc209b40c18833ab814f9af091" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Financing Base Type for detailing generic details of an offer to fund or underwrite something</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="PaymentAmount" id="oagis-id-16b303fd056c4fedadfc0d7ed8a8b270" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Amount of payment</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="BalanceAmount" id="oagis-id-a24ae1c61e534b4a91a934ad7dec11f8" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Balance still owed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FinalAmount" id="oagis-id-c7fe63a5f7f84b29a81597bfcaaf5ca4" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Final Amount to be paid including interest and principle.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ResidualAmount" id="oagis-id-9592d205789d49f1816367a980ae758a" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="PaymentTerm" id="oagis-id-8c0d5113ef544e80937fbc2606d39875" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the Terms i.e. the time and number of payments and the amount or percentage of the payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MaturityDateTime" id="oagis-id-6a9a1a11fbca4ca2b79f2e6b4dec8e51" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Date the loan matures.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FinancialParty" id="oagis-id-1e97ee5df4c5424e8e719414d1cba0fb" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Financial institution that receives the payment transaction from the account owner or authorized party and processes the instruction. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="DownPaymentAmount" id="oagis-id-5fed2e659d92426da60202c12716a62a" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Amount of down payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="TotalAmount" id="oagis-id-9db4f38804354748b309fcc1fd842885" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the total amount of the object associated. This includes the Price and the total of all the charges, allowances and taxes.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PaymentAuthorization" id="oagis-id-ba3cb880c1f14949b673eb85dcc0e35c" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">If the method of payment was authorized (credit Card or EFT), this is the authorization information that was passed back from the 3rd party.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CreditRatingCode" id="oagis-id-3c6036a089724207b1cc863acb653ec4" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Credit rating.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FinancingType" abstract="false" id="oagis-id-9462612122f8421fa6242e4a5a6a6e88" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Financing Type for detailing an offer to fund that can include additional application or process specific information</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FinancingBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="FinancingExtensionType" id="oagis-id-b34772aaa9e141119ae20875914fcdba" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="MarketingProgram" type="MarketingProgramType" abstract="false" nillable="false" id="oagis-id-5bedd250e9cb4952a2c9c018beff4ee7">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the Marketing Campaign that the Lead is eligible for</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="MarketingProgramBaseType" abstract="false" id="oagis-id-c75296092b9248de98019e2963a04644" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Marketing Program Base for detailing the minimum information needed to identify a marketing program</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:group ref="FreeFormTextGroup" id="oagis-id-ffddcd4d23df48db906bf239fd9d7a0e" minOccurs="1" maxOccurs="1"/>
					<xsd:element ref="OfferDescription" id="oagis-id-c54818938efb4fe9885748e72900a254" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Description of the Marketing program offer made by manufacturer</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OpportunityTypeCode" id="oagis-id-48d64e47ec0a45faa22f32cd48a28589" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Marketing opportunities with this customer - Example type -email; phone; postal code</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OpportunityMethodCode" id="oagis-id-014aa210ff054453b228522f1d86b3da" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Method used for marketing opportunity.  Example -Survey/Marketing</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Discount" id="oagis-id-94388df9017c40649cbbcec96bbad38a" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the Discount the time and number of payments and the amount or percentage of the discount from the payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="MarketingProgramType" abstract="false" id="oagis-id-5ed3eae77601495fb1a60097bfe7b5fa" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Marketing Program Type for detailing a program that includes the ability to include application or process specific information</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MarketingProgramBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="Extension" id="oagis-id-ef1f732d9a004a01ad8b6375aa95419e" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!---->
	<!--Financial Components-->
	<!---->
	<xsd:complexType name="CreditBaseType" abstract="false" id="oagis-id-8241039aa4a74282b84e9f66054cd631" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Credit Base Type for detailing an accounting credit minimum information</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:element ref="GeneralLedgerElement" id="oagis-id-e5b6ac1ad7094b709e1096d849017365" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Accounting code elements</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ApprovalStatus" id="oagis-id-58c93ff472cf4813b42f4a9e3e7bd879" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A code to reflect the approval status of a process.  A process may be approved as a way of performing a task.  A non standard process may be approved because of local legal or cultural norms.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="FreeFormTextGroup" id="oagis-id-31a00c2d8e6849cd9e23f246b9e8dee3"/>
					<xsd:element ref="CreditParty" id="oagis-id-df65344259e84807bdea63d7ae4be4c8" minOccurs="0"/>
					<xsd:element ref="Party" id="oagis-id-d7bc126ee12144a083ed336f5f29f88a" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A generic Party  for non-validated or application-validated parties. A work-around for situations where Schema-validation of Parties is not feasible or not desired.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="ReferencesGroup" id="oagis-id-e5c1b26a393249cc9f158c071c2d6ad3"/>
					<xsd:element ref="OpenItemAmount" id="oagis-id-3f1c54fdbe34484babb768e7bd36b772" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the total orders invoiced yet still considered open items in the receivables balance.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OrderAmount" id="oagis-id-30255106f70c4fc98a528497add34fca" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the total Amount of the Order.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ApprovedOrderAmount" id="oagis-id-ae6d8e9b9ecc4947b83e9f5506290482" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the total amount of orders approved but not yet invoiced</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="AvailableAmount" id="oagis-id-f4ae2d45cf564af78715b3d1e17590ab" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the credit amount available for a trading partner to make an additional order against.  This amount is typically a combination of orders approved but not yet invoiced and orders invoiced yet still considered open items in the receivables balance.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OrderLimitAmount" id="oagis-id-4bf14af862014b0fa8cda6af989efe2f" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the credit limit of a single sales order used during credit checking.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="TotalCreditLimitAmount" id="oagis-id-cd9d0ac217394dcd99e5be81faab91ae" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the total credit limit for a trading partner used during credit checking.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="DocumentDateTime" id="oagis-id-60046be6820d444fa90dc39a58b4396c" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The date that document was created within the system of record.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EffectiveTimePeriod" id="oagis-id-935568d3087d4f7ca05d7dfb65ff817f" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the time period in which the associated component is effective.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="LedgerID" id="oagis-id-64ec9006f9824fdb834e55645c7efef7" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the financial ledger to be used when updating balances</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Payment" id="oagis-id-07361374c63b443bb90784fada9ee2f6" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="LedgerEntryHeaderBaseType" abstract="false" id="oagis-id-964e317ecdbe4adbbe7d156816f6b323" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Ledger Entry Header Type for detailing an accounting occurrence Noun general details</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="StatusHeaderBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="LedgerID" id="oagis-id-e0d3bb22017d4558a4571b24b27ab24e" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the financial ledger to be used when updating balances</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="GLEntityID" id="oagis-id-4346aaf0a245451588b9951fe8c43690" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A G/L entity is the smallest organizational unit for which individual financial statements must be drawn up according to relevant commercial law.

It is normally the primary balancing segment of the GL Account structure.  Typically, this is the owning entity for the transaction. 

Synonyms are Business Unit, Fund, G/L Company, Joint Venture, Non-Profit Association Name</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="AccountingPeriod" id="oagis-id-b6be0580144a485eb1d75e16df0e087c" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the date, accounting period and financial year.  It is also known as the effective or post date</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SourceDocumentReference" id="oagis-id-52783267ea4e466f8e0bffabcf68eecc" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the link that ties back to a sub ledger transaction entry ID.  It is the identifier of an original/ source transaction or document.  The type indicates the type of document i.e. PurchaseOrder, Invoice, Receipt, Credit Memo, Voucher etc. For example, it could be the receipt or the summarized inventory activity.  This is the singular field that refers to an audit record.  Together with the Sender information, this is part of the referencing system, which will enable drill back audit trail functionality.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="LedgerAmountGroup" id="oagis-id-e267557caeb1462d9ba8dd0f2693bc15" minOccurs="1" maxOccurs="1"/>
					<xsd:element ref="EntryCreatorUser" id="oagis-id-90d915336fcd4b8497df3c87c1e2eb1e" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the user that creates the associated transaction</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PaymentTerm" id="oagis-id-3ea28ef48f014e8185dc50874906a7d1" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the payment term for an Order or Invoice. Supports the ability to specify an absolute due date, a number of days from a start day, a percent discount or an absolute discount. A common term like "Net30Days" is explicitly code as either a calculated DueDateTime or a PaymentTermsDateTime and NumberOfDays.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Party" id="oagis-id-f360f4dc135d47c5a40b788fbd5d3fc9" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A generic Party  for non-validated or application-validated parties. A work-around for situations where Schema-validation of Parties is not feasible or not desired.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="LedgerEntryLineBaseType" abstract="false" id="oagis-id-d970d08e22d049058ab1dffcad7b17b0" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Ledger Entity Line Type for detailing an accounting occurrence Noun specific details</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="StatusLineBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="GLDestinationEntityID" id="oagis-id-ede326c1220c4b69916433e1046634f4" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">If a value is specified for this and the value is different from the GLSourceEntity, it indicates that the entry is an inter-company transaction</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="LedgerAmountGroup" id="oagis-id-e7903ad2cfdf472b9789958ce6699fb2"/>
					<xsd:group ref="LedgerEntryContentGroup" id="oagis-id-9145f0def03748ad9c07f667e3d78a51" minOccurs="1" maxOccurs="1"/>
					<xsd:element ref="ReasonCode" id="oagis-id-5da2a9c440114ca9bcb365f5626d4b29" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the reason for an activity</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="WarehouseParty" id="oagis-id-40423430e70e4ef683ddc669666d8b9c" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="BuyerParty" id="oagis-id-4dddcce73a084151a3c9cc52b32070f0" minOccurs="0"/>
					<xsd:element ref="TaxJurisdicationCodes" id="oagis-id-41c2d0a2682f4377b6fca44cbd2791a5" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="LedgerEntryContentGroup" id="oagis-id-9a837202183444d6bbf8876bbe1b5b61">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Group to assemble and enable rapid reuse of accounting basic information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="GeneralLedgerAccount" id="oagis-id-c6634914ded348efad04e727723d889f" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the GLAccount for the associated document and component.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ProjectReference" id="oagis-id-0b87dd1e3e6c43f6973855c5a30d0196" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies a project that is associated with the given component.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Item" id="oagis-id-53d1bd35d6df4dd3ab53b2e3d19bd5bf" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the Item associated with the Line or Detail of a transaction. The item provides the details of the generic Item component information. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="ItemInstance" id="oagis-id-6c202555805b414abf6a3fb33f5a228c" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="Quantity" id="oagis-id-05b3bbfb53d74198af84245b24d25ae2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the quantity of the associated service or item that is addresses by the component in which the quantity is associated.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="EnterpriseUnit" id="oagis-id-f278f4d32fa542fda8484d16be143c0e" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the a given portion of an organization.

Some commonly defined  Units are identified here.

These units are typically used to track financial data in reporting systems within an organization. Similar to Business Units, Organizational Units are only  relevant within an enterprise</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Employee" id="oagis-id-b2814319d2ae4769bc8fd7da28365bf7" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Employee information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="EntryCreatorUser" id="oagis-id-432f1f84ceb94d76ae302412e7b825db" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the user that creates the associated transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Party" id="oagis-id-53e1049e7b4b4bd68beeb36894add8a5" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="TransferPaymentBaseType" abstract="false" id="oagis-id-a91087c7a1454ba590bf57232a89e092" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Transfer Payment Type for detailing an exchange of funds</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="RequestedExecutionDateTime" id="oagis-id-5a8a60307ad7474096de045b05c6ac75" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Date at which the initiating party requests that the payment instruction be executed. If payment by check, the date when the check is required to be generated by the bank. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FirstAgentPaymentMethodCode" id="oagis-id-0549e24f81244b4293c77700dcd2d4fb" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Specifies the transfer method that will be used by the first agent to transfer the funds to the creditor.

IST limits these values to CreditTransfer (TRF) and Check (CHK).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="DebtorParty" id="oagis-id-dc6ef76208e446c9b7dc792bb4e2061e" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Party that owes an amount of money to the creditor. In the context of the payment model, the debtor is also the debit account owner.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FirstAgentParty" id="oagis-id-0eedf3319a444451abc26c73640fe7da" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Financial institution that receives the payment transaction from the account  owner or authorized party and processes the instruction. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Party" id="oagis-id-164025e3f2ba4e6391d2e96598a52d06" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:group ref="FreeFormTextGroup" id="oagis-id-373c7bf2c11641058902dc4a4c91d709" minOccurs="1" maxOccurs="1"/>
					<xsd:element ref="PaymentTransaction" id="oagis-id-b2c74a5906584c0bbcdd4ac52bd68b7b" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information to communicate the payment transaction.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TransferAgreementBaseType" abstract="false" id="oagis-id-b499f411a43d485480de92cdbf0741b1" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Transfer Agreement Type for detailing a funds exchange contract</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:element ref="CreditTransferType" id="oagis-id-0d15d2d759124a9a8a68ecf73f6ad783" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is used by agents in the payment chain to define the instrument and/or processing conditions that will be used to execute the payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Proprietary" id="oagis-id-475d99a98085461f9ecd0f473d0d82b9" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Provides a proprietary text description.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PriorityCode" id="oagis-id-577f72f9123b4794bd153396995745ad" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the priority or urgency of the associated entity. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="SettlementPriority" id="oagis-id-d1d3d0943a544870920951e2bc3378b7" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the priority with which or the system through which the agents in the payment chain will clear and settle the payment transaction.

IST requires the PriorityCode to contain one of two values: 
HIGH - Indicates a priority level of high.
NORM - Indicates a priority level of normal</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="TransferAgreementType" abstract="false" id="oagis-id-b8511aa3c52c46b6b28f12bd85365fca" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Transfer Agreement Type for detailing a funds exchange contract</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="TransferAgreementBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="TransferAgreementExtensionType" id="oagis-id-b970dc45939c4728a185499b25391a53" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="DebitTransferAgreement" type="TransferAgreementType" id="oagis-id-56a173dc07fa4a65a653b36290e8fab1">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Set of elements that further identifies the type of credit transfer being requested by the initiating party.

Business Rule: This component may be present if payment method is TRF. It must not be present if payment method is CHK.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!---->
	<xsd:complexType name="PaymentTransactionBaseType" abstract="false" id="oagis-id-6198a12a462d4c6e8c69cb349ae43e33" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information to communicate the payment transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:group ref="PaymentIDGroup" id="oagis-id-ebd85f22f0254c4e9ff7e5e7992499ce"/>
					<xsd:element ref="PaymentPurposeCode" id="oagis-id-61d53f7099a64170825b9f241ab633be" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The underlying reason for the payment transaction, e.g. a charity payment, or a commercial agreement between the creditor and the debtor. Provided by the code contained.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Proprietary" id="oagis-id-6d9ef8d715f64ab88ba85904116e735a" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Provides a proprietary text description.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="InstructedAmount" id="oagis-id-9a70ecc67dcd4910a1a543614dc4d0cc" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Amount of money to be transferred between debtor and creditor, before deduction of charges, expressed in the currency of the debtor's account or in another currency.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EquivalentAmount" id="oagis-id-ce37c22ee8164460a7140594e4634287" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Amount of money to be transferred between the debtor and creditor, before deduction of charges, expressed in the currency of the debtor's account and to be transferred into a different currency. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CheckInstruction" id="oagis-id-1cc2b80a543e4cc8bc6dccd0285b4b10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information related to a check instruction. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OriginatorParty" id="oagis-id-539de52178b94f539102e9fae6bcf527" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that originated the purchase of the goods, service or payment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="IntermediaryFinancialParty" id="oagis-id-b2975d6be80540518875eb701411dc74" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">An agent between the first and final agent.

Usage guideline: This information is provided by the initiating party, based on information provided by the creditor or final party. How this information is handled by the first agent will depend on the service level/business practice between initiating party and first agent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CreditorParty" id="oagis-id-6140ee5e206c4023aa86f5968c4936f9" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Party that receives an amount of money from the debtor. In the context of the payment model, the creditor is also the credit account owner.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FinalAgentFinancialParty" id="oagis-id-351d5bc7442942338868142ea1d333b8" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Financial institution that receives the payment transaction on behalf of an account owner, and posts the transaction to the account.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FinalParty" id="oagis-id-d8387e0cd99d438bb24cac21f4d47eb9" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Party that is the ultimate beneficiary of the cash transfer.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ChargeBearerCode" id="oagis-id-5c62050ba6064564886bad97607dfdcb" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Specifies if the creditor and/or debtor will bear the charges associated with
the processing of the payment transaction.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ExchangeContractReference" id="oagis-id-6a44a90fb393482383e66aaf760cd9d4" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Unique and unambiguous reference to the foreign exchange contract agreed between the initiating party and the first agent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RegulatoryReporting" id="oagis-id-18d865d92a27449fba6677afb16fdb62" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information needed due to regulatory and statutory requirements,.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FinalAgentInstruction" id="oagis-id-ecfc112359e5407eaa1e0005e25e83d3" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Further information related to the processing of the payment instruction, provided by the initiating party, and intended for the final agent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FirstAgentInstruction" id="oagis-id-d56c8d886f914a6699c64b54bf675ae6" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Further information related to the processing of the payment instruction, which may need to be acted upon by the first agent, depending on agreement between customer and the first agent. The instruction can relate to a level of service between the
customer and the first agent, or can be information required by the first agent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Remittance" id="oagis-id-287d92079aad43a891d6e8c0cdfeb036" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information supplied to enable the matching of an entry with the items that the payment is intended to settle, eg, commercial invoices in an Accounts Receivable system.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="FreeFormTextGroup" id="oagis-id-0f916a5b605e4759a0b6519caabb5394"/>
					<xsd:element ref="Status" id="oagis-id-4f49d08bb67c4b3ebd928f99fc3b7f8c" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the status of the associated object by providing the Status Code along with a description and when the status is effective.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RejectionStatus" id="oagis-id-c152282c60b64480a29a507a2d599005" minOccurs="0"/>
					<xsd:element ref="Party" id="oagis-id-c9393193137e45858430986238584640" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
				<xsd:attribute name="sequenceNumber" type="NumberType_B98233" id="oagis-id-2e36354311e54122af490427db8a8d8f">
					<xsd:annotation>
						<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">When an object occurs multiple times, this sequence number can be used to provide the order.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PaymentTransactionType" abstract="false" id="oagis-id-d76164bd3d354bceb991f2850616b674" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information to communicate the payment transaction.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PaymentTransactionBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="PaymentTransactionExtensionType" id="oagis-id-b7b7e81f58cc4ea09c890b2ebf913fc0" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="PaymentTransaction" type="PaymentTransactionType" abstract="false" nillable="false" id="oagis-id-a296eec689fe4c83b8835744a8b8ccaa">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information to communicate the payment transaction.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:group name="PaymentIDGroup" id="oagis-id-5b4f74b992a64857bbbac007a1ec5fe0">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Group of elements used to provide further means of identifying the payment transaction. </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="InstructionID" id="oagis-id-81eedbfe4d464fc8861ba369f623b6c6" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Unique and unambiguous identifier for a payment instruction assigned by the initiating party.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="TransactionID" id="oagis-id-c23d80614a3345319b820f789d170dde" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Unique and unambiguous identification of a payment transaction, as assigned by any of the parties on the initiating side, which will be passed on throughout the entire end-to-end chain.   </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="RemitID" id="oagis-id-7a5da848a842493eafd21a9a251de7e2" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Unique and unambiguous identification of remittance information which is sent separately from the payment initiation information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="RegulatoryReportingBaseType" abstract="false" id="oagis-id-e552d948234c4ad9ab9039e05419ddcd" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information needed due to regulatory and statutory requirements.

Code - Specifies the regulatory reporting code.

Amount - Amount of money and currency that may need to be reported for regulatory and statutory requirements.

Note - Or Information - Additional details that cater for domestic regulatory requirements.

</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:element ref="Code" id="oagis-id-6b5ae8a034214ce1af3113ece46af1fe" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Element for the communication of all codes.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Amount" id="oagis-id-4f51d2583c7d40839fe8bc10072787eb" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The monetary value of the object in which the amount is associated</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Note" id="oagis-id-38c9797148994a7abf0d808767908267" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
