			<xsd:extension base="URIType">
				<xsd:attribute name="format" type="EMailFormatCodeContentType" use="optional" id="oagis-id-f55c61c514d84094a57335b01d1348f3"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="EventBaseType" abstract="false" id="oagis-id-56adc9de89f642abaa1aa806f18dc0e3" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Event Type for specifying information about an identifiable occurrence in time</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:element ref="StartDateTime" id="oagis-id-c9be56cda1f24cbc97d5cd77c3f600b0" minOccurs="0"/>
					<xsd:element ref="Duration" id="oagis-id-778f1fd93b2b4e3ea0dec874c96e1f3d" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the duration of time as represented by ISO 8601. Where the value space is a six dimensional space where the coordinates designate the Gregorian year, month, day, hour, minute, and second. The number of seconds can include decimal digits to arbitrary precision.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EndDateTime" id="oagis-id-1849aa6efa544f6583482490f4bdc752" minOccurs="0"/>
					<xsd:element ref="Description" id="oagis-id-e7b383bc0e824dfca3a0be7269a27d94" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EventType" abstract="false" id="oagis-id-2df003409bc14c1a9f4363f72d81a1a0" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Event Type for specifying information about an identifiable occurrence in time</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="EventBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="EventExtensionType" id="oagis-id-1c85c941fa24425a89caba743cd8f27f" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="Event" type="EventType" abstract="false" nillable="false" id="oagis-id-30857d6c465e450e8a87552ab6d0b4d2">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies details about a specific occurrence in time</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AmountDiscountBaseType" abstract="false" id="oagis-id-58778be6c97d4f18ac2e2b6d1ad3ecbb" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Qualified Amount Type for specifying amounts</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:element ref="DiscountAmount" id="oagis-id-a0196d58157d46a9a66e8ed5056a92ac" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates an amount of discount that is available against a price</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Amount" id="oagis-id-df674a115e99441280ddcf0a0221dba0" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The monetary value of the object in which the amount is associated</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Percentage" id="oagis-id-d15dda174c9640ef90c7f24d65e81e92" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the percentage of the Line Price applicable to the distribution line. This is used to derive the Amount on the distribution line.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Description" id="oagis-id-b230bde0349a4130ae7d3682d05f27f2" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
