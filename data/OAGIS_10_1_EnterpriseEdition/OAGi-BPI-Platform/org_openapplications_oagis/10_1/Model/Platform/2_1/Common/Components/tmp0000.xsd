		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is used to describe preferences that will be used to evaluate responses to an RFQ. For example, to identify a preference for sharing CAD data in VRML format the ID = VRML, Type = CAD and Value = 1 or Yes</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="OrderCommissionBaseType" abstract="false" id="oagis-id-24fdb4f3e64c4f8d91f2178674f08fbe" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Contains the relevant information about the salesperson and the associated commission information. </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="IdentificationType">
				<xsd:sequence>
					<xsd:element ref="SalesOrganizationIDs" id="oagis-id-f9f8f5ff71a74214b42e8a2f77cc7090" minOccurs="0"/>
					<xsd:element ref="SalesPerson" id="oagis-id-73eba4022b824722bb34f213b694cd78" minOccurs="0"/>
					<xsd:element ref="Amount" id="oagis-id-e6a41091a8034a8b8702ab12bb5ee91a" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The monetary value of the object in which the amount is associated</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Quantity" id="oagis-id-5fda0b994c704c86abcf4b1d547017c7" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the quantity of the associated service or item that is addresses by the component in which the quantity is associated.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OrderAmount" id="oagis-id-02deaf1d3acd4f2794e291bdb79da150" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the total Amount of the Order.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OrderQuantity" id="oagis-id-a802a9a352d44cdb8018f28bfd295152" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the quantity of a material or product ordered by the customer.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Percentage" id="oagis-id-2c366ed3e1aa4efcb5682b72ac0e0f64" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the percentage of the Line Price applicable to the distribution line. This is used to derive the Amount on the distribution line.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="OrderCommissionType" abstract="false" id="oagis-id-11a7e221b3894c688f2bb30d0e1316a0" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Contains the relevant information about the salesperson and the associated commission information. </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="OrderCommissionBaseType">
				<xsd:sequence maxOccurs="1">
