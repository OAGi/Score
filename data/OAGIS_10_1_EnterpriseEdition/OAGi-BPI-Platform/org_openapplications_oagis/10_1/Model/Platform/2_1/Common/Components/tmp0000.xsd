					<xsd:element ref="UnitAmount" id="oagis-id-96d08719773f4fcfa4d7d40e9d50e1df" minOccurs="0"/>
					<xsd:element ref="TimePeriod" id="oagis-id-6150fa9a86c948dca60985828c46c567" minOccurs="0" maxOccurs="1">
