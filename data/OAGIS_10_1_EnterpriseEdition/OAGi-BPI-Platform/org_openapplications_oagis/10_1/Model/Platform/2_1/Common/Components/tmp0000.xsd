	<xsd:element name="BillToParty" type="CustomerPartyType" abstract="false" nillable="false" id="oagis-id-2e52f55be83245299f0c6f13c5feee54">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that the bill or invoice is to be sent. In most cases this is the same as the CustomerParty however there are many cases where the CustomerParty and the BillToParty will be different.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="BranchParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-982e3f76302d4869a5aeae9276a9d5f5"/>
	<xsd:element name="BuyerParty" type="CustomerPartyType" abstract="false" nillable="false" id="oagis-id-7da061d1ecd04284b46ad1bb4d81471c">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The entity that is responsible for Buying the document or element in which it is associated. Additionally, the contact  may be a person or a group or department.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CarrierParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-e9bfb4eb30054e3db24b43bca140b73f">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that serves as the carrier of the goods or services for the transaction.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CatalogParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-74a98ef54e814d1097cdd8b1e25b388e">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party whose catalog is used to purchase the goods or services.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CheckFromParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-1dcd0add9ea34de6a18962e05dc574cf">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that ordered the issuance of the check.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ChildParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-48ea46bca52b40abb7d5fa358b943f2b"/>
	<xsd:element name="CleanedByParty" type="PartyType" id="oagis-id-e3c043f1748c4e56a33e880e37b90650"/>
	<xsd:element name="CompetitorParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-2d149f834a874dc6b20dfdd355adf46d">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the organizations that are competing for this business.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ConcernedParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-31d179a03e744258b5a26493add18980"/>
	<xsd:element name="ConfigurationEndUserParty" type="PartyType" id="oagis-id-7954013976b54c15b8f1d6b470bff722"/>
	<xsd:element name="CreatedByParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-d27e527dd13a41cf85100bcc2e5b2c5f">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the  organization that created the cart. In a third party buying scenario, this will identify the Party that created the cart on behalf of another Party</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CreatedForParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-5ecc830111354df68b571eb82f0b5adc">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This identifies the organization/ entity for whom the cart was created. This is also used in the context of third party buying, where this will identify the entity or organization for whom the cart is intended</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CreditParty" type="PartyType" id="oagis-id-a0f05a3cef794004b9c31b9c4bdb1a99"/>
	<xsd:element name="CustomerParty" type="CustomerPartyType" abstract="false" nillable="false" id="oagis-id-c19a8303c96443e3a831e755db3ec16c">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the organization that is the end customer of the goods or services being provided in the associated transaction.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DeclarantParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-03a9ef234b4b41cfbe247d1646412c8b"/>
	<xsd:element name="DeliverToParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-ebe21591d3d04dcb9640f4704d5cb82f">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the Party (the person or organization) to receive delivery of a the associated entitles.  This can be goods, services, checks, etc.

Several examples of the value of this field include an office number, a complete address, or a person</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DepositorParty" type="PartyType" id="oagis-id-4a6bc43832ab46c995f6cf89a2473a01"/>
	<xsd:element name="DistributorParty" type="PartyType" id="oagis-id-1c6861a0c12b4f6eb93805618ccde9b6"/>
	<xsd:element name="EndUserParty" type="PartyType" id="oagis-id-b7e3473aa7a145ff81a45bf9df018212"/>
	<xsd:element name="FinalAgentFinancialParty" type="FinancialPartyType" abstract="false" nillable="false" id="oagis-id-326af970bdf141a3a7217a5627415240">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Financial institution that receives the payment transaction on behalf of an account owner, and posts the transaction to the account.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FinalParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-3075dc63f19f47e1b945b5b8317ca347">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Party that is the ultimate beneficiary of the cash transfer.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FinancialParty" type="FinancialPartyType" abstract="false" nillable="false" id="oagis-id-08d443cf01cd47a6869728106dcc9bd0">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Financial institution that receives the payment transaction from the account owner or authorized party and processes the instruction. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FirstAgentParty" type="FinancialPartyType" abstract="false" nillable="false" id="oagis-id-5b9005e2f6284fb79dedd1d96f3121cb">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Financial institution that receives the payment transaction from the account  owner or authorized party and processes the instruction. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ForwardingAgentParty" type="FinancialPartyType" abstract="false" nillable="false" id="oagis-id-aa2987d3fb1b4dc3985403e66ea7e37f">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Financial institution that receives the payment transaction from the account owner or authorized party and processes the instruction. </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InitiatingParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-cb0c1e571f024349a98d95c183279e68">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Party initiating the payment to the forwarding or first agent. This can be the debtor (in a credit transfer), the creditor (in a direct debit) or a party that initiates the payment on behalf of the debtor or creditor.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IntermediaryFinancialParty" type="FinancialPartyType" abstract="false" nillable="false" id="oagis-id-3d6df9bfc43740c9857c36037fa6a6b1">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">An agent between the first and final agent.

Usage guideline: This information is provided by the initiating party, based on information provided by the creditor or final party. How this information is handled by the first agent will depend on the service level/business practice between initiating party and first agent.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InvoiceeParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-9d1db9d447c24723b797ecd6e5da75bd">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">PmtInf/PmtTx/RmtInf/Invcee</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InvoicerParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-5df73f78e6d042db910817d23a5e2663">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">PmtInf/PmtTx/RmtInf/Invcr</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="IssuingParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-df491ff960104cfa89d517f2eae5c1d5">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the organization/entity that issued/ created the document</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ManufacturingParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-48d24f6ecd404a2486f338bc4088d0e7">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that manufactured the associated Item.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OriginatorParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-f9fd53d4c8d04f5bac167494a3428afd">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that originated the purchase of the goods, service or payment.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="OwnerParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-5ba54e6772414a0f810a7713f51fcb72">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is used to identify who or which organization owns the item or part. The content of this field is user defined based on a specific Customer or Supplier.

Synonyms are Vendor Consignment ID, Supplier Consignment ID</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ParentParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-884c316ae19545edbc58e7ced56caa6d"/>
	<xsd:element name="PayeeParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-087b974320c7418db2b57ff70d0ade3f">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is a Party that is identified as a Payee on an Accounts Payable document. The SupplierParty of the goods or services typically.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="PayFromParty" type="PartyType" abstract="false" nillable="false" id="oagis-id-6a70207b2cf941228677cf25ee41b3fd">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that the payment for the goods or services will come.</xsd:documentation>
