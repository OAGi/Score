						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="TransportStep" id="oagis-id-0f3d6e671fbc487cadb093b31cb1c999" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="PaymentTerm" id="oagis-id-f5389e3e14c04cb3bd132e302695c565" minOccurs="0" maxOccurs="unbounded">
