	<xsd:complexType name="PortType" abstract="false" id="oagis-id-a430284a73164ee198ab3ea2778489e3" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Port Information Type for specifying port details</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PortBaseType">
				<xsd:sequence>
					<xsd:element name="Extension" type="PortExtensionType" id="oagis-id-72f373d7cda24ad98d99f0a452fb0882" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="Port" type="PortType" abstract="false" nillable="false" id="oagis-id-2ffbef00f27848c28329fc9631c09fb6">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies details for a specific logistical location</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ShippingRoute" type="ShippingRouteType" abstract="false" nillable="false" id="oagis-id-e7e85a80745947779184fe8dd5b0f40b">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies details for a logistical segment of a transport process</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HazardousMaterialShipmentContainer" type="HazardousMaterialShipmentContainerType" abstract="false" nillable="false" id="oagis-id-cb0724a6e80e4c7f900a6eb9ff0923f9">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies details for both a container and its contents containing hazardous material</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="CustomsExport" type="CustomsExportType" abstract="false" nillable="false" id="oagis-id-55474308503745ca995dbe56ac5e205f">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies details for a specific permitted export</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ShippingLabel" type="ShippingLabelType" abstract="false" nillable="false" id="oagis-id-087b9ff7eb704e429a44ee8bec301425">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies details for a label used for logistics</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!---->
	<!--Manufacturing Components-->
	<!---->
	<xsd:element name="ManufacturingItemInstance" type="ItemInstanceType" id="oagis-id-c7ecf15bc1bd4bdf8f694dcc3bee6840"/>
	<xsd:element name="FinishedGoodItem" type="ManufacturingItemType" abstract="false" nillable="false" id="oagis-id-2a9c7e93cdbc4d03bfab9b6deac141a7">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the finished good en item. In the case that the Item is raw material or sub-component. This identifies the finished good that it is for. For example in case of Manufacturing an airplane and the components used must be tracked.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:group name="LotSizeGroup" id="oagis-id-b67a8e0d30474f9f99bb34248ce8e3e2">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Group to access details of how a lot is sized</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="MaximumLotSizeQuantity" id="oagis-id-92f3f6cd4af74a12884449a7e5d2557f" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the maximum number of items that can be put into a lot for this Item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="MinimumLotSizeQuantity" id="oagis-id-a3eb11273e394ad79a039b65af751563" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the minimum number of items that can be put into a lot for this Item,</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="LotSizeMultiplerNumber" id="oagis-id-89d5a2096d514b9fa768c15863d4db3d" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is used with the MinimumLotSize and MaximumLotSize.  It contains the multiplier between the minimum and maximum manufactured lot quantities.  For example, if the maximum lot quantity is 10,000 and the minimum lot quantity is 1000, the lot size multiplier could be 1000.  This means the Item may be manufactured in lot sizes starting at 1000 and going up to 10,000 by 1000 increments.For example, 1000, 2000, 3000, etc.  A lot of 4500, would not be allowed under these constraints.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="ManufacturingItemBaseType" abstract="false" id="oagis-id-94a5e60008dc492d9f2a10c8495db08b" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Manufacturing Item Base Type for standards based detailing of a manufactured item</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ItemInstanceBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element ref="Effectivity" id="oagis-id-b509dbe551864f8db2e6163a135333fb" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies effectivity of an associated object. Effectivity is expressed may be expressed as: multiple ranges, values, time period or as associated to a specific Item.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EffectiveTimePeriod" id="oagis-id-d5a37209d27e495b81bbf442c66b7e7e" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the time period in which the associated component is effective.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ExecutionTimePeriod" id="oagis-id-3bb3e91cdf094c0fada69aa9b8f3605d" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the time in which the associated entity is actually in process.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FixedQuantityIndicator" id="oagis-id-76bc2eebc274460cb89d131c70b95fd6" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates whether the ItemQuantity is a fixed size and does not scale along with the quantity of the BOM or Routing being produced</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="LotSizeGroup" id="oagis-id-0fb512015d074bfaa13469176102110a" minOccurs="1" maxOccurs="1"/>
					<xsd:element ref="LeadTimeDuration" id="oagis-id-1fbd7747b99e4bf4989f9c650e76ccca" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the duration of time that is required to add to a manufacturing process to forecast availability of this Item.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RequiredPercent" id="oagis-id-a8637a08860642c8acbefde39c839071" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the percentage of the total quantity on the Bill of Material that is required to be available before the manufacturing process can begin</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="BackFlushedIndicator" id="oagis-id-ea89d11fc68c413ca00bf4986db9de72" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates whether the associated quantities are back-flushed or not.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ActualIndicator" id="oagis-id-ec943e6ea5ac4e159a376e94768e9814" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates if the associated quantities are reported as they happen.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="TrackingIndicator" id="oagis-id-7bfbe2ff09bd464ea8b06687a0377938" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates whether the related object is to be tracked</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ScrapIndicator" id="oagis-id-ec9bbad78ec54bd08153e4cc041c3848" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the status of an item as a scrapped item</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ManufacturingItemType" abstract="false" id="oagis-id-b682e02faff443388235d9fec07bbdbd" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Manufacturing Item Type for adding application or process specific to a manufactured item base</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ManufacturingItemBaseType">
				<xsd:sequence minOccurs="1" maxOccurs="1">
					<xsd:element name="Extension" type="ManufacturingItemExtensionType" id="oagis-id-7ed640bb4cb149fab499945e7d41348b" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Allows the user of OAGIS to extend the specification in order to provide additional information that is not captured in OAGIS.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is done by defining the additional information in XML Schema and referencing the new schema in the xml instance document through the use of namespaces. Once this is done the additional information defined there can be carried in the BOD XML instance document.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The Open Applications Group will make best efforts to quickly consider all proposed submissions.The Extension is always the last element in all components, except where the component has been extended inline.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="ManufacturingItem" type="ManufacturingItemType" abstract="false" nillable="false" id="oagis-id-84358baf03bf4b488a1e71a6c3e4873c">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the manufacturing view of an Item where the complete detail of an Item is represented by the MasterItem.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:group name="OperationGroup" id="oagis-id-8146b056f573479da6f84a734becbaad">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Group of Operation elements that occur together in the Operation and QualifiedResource.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="BatchSizeQuantity" id="oagis-id-fc07db1798e24dc7a39857bdcf15fcff" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the quantity of a product or item made during a batch production run or the amount of material produced by the Operation being defined.  An example would be how many cookies produced by a particular Operation..</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PerishedInOperationQuantity" id="oagis-id-067186fc4e9f4ec39fdd0b0b70d6a631" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the number of items that have perished within the operation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="PerishedBetweenOperationsQuantity" id="oagis-id-3c5586be2c7647cf88465d88433c8d41" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the number of items that have perished between operations.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="TransferLotQuantity" id="oagis-id-5ec6c9c09c08422d9878025b340507b8" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is quantity to be transferred between operations within one lot.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Quantity" id="oagis-id-5e7c3bdd1d4c45f9bac5a22e00679d0c" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element ref="SetupTimeDuration" id="oagis-id-8db8d9982276432e8e52e4f2632ff287" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the time it takes to setup to run the operation. This is a duration time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="WaitTimeDuration" id="oagis-id-426e74ad70264ca1a8f37d1c2ab4b842" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the time that an operation can wait after being run. This is a duration of time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="RunTimeDuration" id="oagis-id-09d88e6cdd5449309e2c3ab0889b8301" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the time per item to perform the specific operation.  This is a duration of time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="TearDownDuration" id="oagis-id-f40bbe0ff93a4a5ba5efd3fc00f45e85" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the time it takes to tear down from running an operation.  The time it takes before setup for another operation can be performed. This is a duration time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="BatchDuration" id="oagis-id-8fe96ada27f64b5ca7cf8be1823c4c7a" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is a fixed duration of time indicating the time it takes to perform a batch.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="QueueDuration" id="oagis-id-96f2a2267bc244f08dcce3fde5b0ae5c" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the time the job may be in the queue before being able to be run at the work center location. This is a duration of time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="MoveDuration" id="oagis-id-9807016d6b0f481a93381ae8351198e3" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the time it takes move the part from one work center location to another work center location. This is a duration time.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="FixedDuration" id="oagis-id-2e3665204317493a9e633550158b76d0" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is a fixed duration of time where the time it takes to perform is known and fixed prior to the operation being performed.  An example of this would be something sent out to be worked, where the duration of the process is known before it actually happens.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="RejectPercent" id="oagis-id-6cf88790cec640eb9eb9f2282bb3d412" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the percentage to be rejected.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Effectivity" id="oagis-id-7f9b798155fe4bbf854a73e6198e5223" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10">Specifies effectivity of an associated object. Effectivity is expressed may be expressed as: multiple ranges, values, time period or as associated to a specific Item.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="OperationBaseType" abstract="false" id="oagis-id-0b5e91c89e66486393652186165c6dc4" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Operation Type for detailing a manufacturing step or operation</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="HeaderBaseType">
				<xsd:sequence>
					<xsd:element ref="BOMReference" id="oagis-id-99c25ce4f7d34c8caec3d16afa744858" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is a reference to a Bill-Of-Material Document.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RouteReference" id="oagis-id-3401de7f2c6b4398909d5eb6e7a70e9d" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Reference to a Routing.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CostTypeCode" id="oagis-id-db740c1b4bac4537b32abacaa03fe3a6" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the.methodology by which the value of an item is determined.

Values are:
Standard
Moving Average
LIFO - last in first out.
FIFO - first in first out</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ContainerTypeCode" id="oagis-id-883eb37a033a4efbaf6e36d9da721b27" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the type of container used to place the items that result from the operation.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="DepartmentID" id="oagis-id-15d961771d244666b5b1607de08e6c34" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the department in which the associated entity belongs within the given organization. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CostAmount" id="oagis-id-b61866d349dd422a95329aae17ef49f2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the cost of the given entity on the General Ledger As such the currency used is the currency of the General Ledger. In the case of an operation it the cost of performing the Operation.</xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the cost of the operation on the General Ledger As such the currency used is the currency of the General Ledger.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ProcessCode" id="oagis-id-fb759468756347a4bc8ccd9989d02399" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is a process code used for grouping like operations.
Possible examples are:  Heat Treat, Dyeing</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MachineSetupDependencyCodes" id="oagis-id-0132b10116764388a808cc60d11f9d8f" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates whether the machine can be setup for the operation early or whether setup must happen at a specific time depending upon conditions.

Possible values:
- Early	
- NotDelayed	
- DelayedByPredicessor
- DelayedByMaterial</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MaximumParallelTeamsFactorNumber" id="oagis-id-ce5323cc219944608f701beeb8e19fba" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Defines a maximum number of parallel work teams allowed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FixedRejectQuantity" id="oagis-id-795aec093ac44c9b8dc751c06d0cb5be" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The fixed number of parts that will be rejected during setup, and tear down of an operation.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RejectedQuantity" id="oagis-id-7a0f6dd980de4871a3276d945f63d01c" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the quantity rejected for the specified Reason</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="OperationGroup" id="oagis-id-bcf52062912a41f597abe25c1e030719"/>
					<xsd:element ref="SiteLocation" id="oagis-id-8775719bb46a4fc58273f628619a33cf" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the Site Location that the associated entity occurs.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="MultipleRunSaveTimeIndicator" id="oagis-id-67b93d9b84674886b79019e8545485d4" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates that time can be saved by running this operation multiple times.  </xsd:documentation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10"/>
						</xsd:annotation>
					</xsd:element>
