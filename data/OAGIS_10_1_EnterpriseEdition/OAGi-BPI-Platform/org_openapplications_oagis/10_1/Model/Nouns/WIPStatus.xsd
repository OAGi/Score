	<xsd:include schemaLocation="../Platform/2_1/Common/Components/Components.xsd"/>
	<xsd:element name="WIPStatus" type="WIPStatusType" id="oagis-id-e48b36ab7a7545918507edd21a833e90">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">WIPSTATUS is used to notify a Manufacturing Application of the progress of a production order at a point in time.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="WIPStatusBaseType" id="oagis-id-f2f2bfeb5ee24c9ea97e76719b9db8f4">
		<xsd:complexContent>
			<xsd:extension base="WIPEventHeaderBaseType">
				<xsd:sequence>
					<xsd:element ref="DueDateTime" id="oagis-id-f7b0b615c2464fe7a39729a74aafde11" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The date and time that the production order is to be completed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ScheduledQuantity" id="oagis-id-ca315b3611a14950a2a34d825e9b84e5" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the number of items that was scheduled to be produced.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="DueQuantity" id="oagis-id-5ad741ffa7794fc1a4843caf5af697a5" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The number currently due or scheduled to be completed.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Operation" id="oagis-id-33dee69bc31a441b97359fbb5ac6e362" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Information that describes the operation to be performed. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="typeCode" type="CodeType_1E7368" use="optional" id="oagis-id-5e531522ca4145f194849dde35d19a84"/>
				<xsd:attribute name="actionCode" type="ActionCodeContentType" id="oagis-id-d8f8521dab5846b582d8f792ffc9b598"/>
