<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.openapplications.org/oagis/10"  targetNamespace="http://www.openapplications.org/oagis/10" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:include schemaLocation="../Platform/2_1/Common/Components/Components.xsd"/>
	<xsd:element name="HazardousMaterialShipmentDocument" type="HazardousMaterialShipmentDocumentType" id="oagis-id-5731ec1035d74c03a41b64c291ac50e4">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">A HazardousMaterialShipmentDocument identifies the hazardous material on a shipment.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HazardousMaterialShipmentDocumentHeader" type="HazardousMaterialShipmentDocumentHeaderType" id="oagis-id-df620acf99fe4999a3a077c898b5accd">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Provides general information about the document that is used to transport a specific quantity of material goods from a supplier to a single customer business partner destination.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="HazardousMaterialShipmentDocumentLine" type="HazardousMaterialShipmentDocumentLineType" id="oagis-id-c2ea7bcaab6640b39264a79443df2f99">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Provides general information about the item inventory being delivered, including the planned and actual quantities.  This information usually directly references a line item or shipment schedule line on a sales order, purchase order or other inventory transfer document.  </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="HazardousMaterialShipmentDocumentType" id="oagis-id-4f656f813e584d32a647be5ab20b0ff0">
		<xsd:sequence>
			<xsd:element ref="HazardousMaterialShipmentDocumentHeader" id="oagis-id-a9c415a02c4849a7b24a5f6aa7d6ee00" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10">Provides general information about the document that is used to transport a specific quantity of material goods from a supplier to a single customer business partner destination.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="HazardousMaterialShipmentDocumentLine" id="oagis-id-77b704e2e2fe4a5a8fcc8f0d511d858b" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10">Provides general information about the item inventory being delivered, including the planned and actual quantities.  This information usually directly references a line item or shipment schedule line on a sales order, purchase order or other inventory transfer document.  </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="typeCode" type="CodeType_1E7368" use="optional" id="oagis-id-06e0db6c7eba497690217b7adee6295c"/>
		<xsd:attribute name="actionCode" type="ActionCodeContentType" id="oagis-id-65240590a8fc4bd28fff42cbe250f342"/>
	</xsd:complexType>
	<xsd:complexType name="HazardousMaterialShipmentDocumentHeaderBaseType" abstract="false" id="oagis-id-8d68bc8ac43945ecb19413e2dc5688d5" mixed="false">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Hazardous Material Base Type for detailing the minimum applicable details of a hazardous material Noun</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="StatusHeaderBaseType">
				<xsd:sequence>
					<xsd:element ref="ContainerQuantity" id="oagis-id-d0662ba7995b4df4a089a3345aee0e53" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A numeric expression of the number of outer containers for product shipping.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Port" id="oagis-id-39199e0dcdbe492c90064b7cc88d0124" minOccurs="0"/>
					<xsd:element ref="ActualShipDate" id="oagis-id-4631a79071994ac6a43ec1f4f8c69144" minOccurs="0"/>
					<xsd:element ref="GrossWeightMeasure" id="oagis-id-e527d827dbc74005be7540cd83e6fbe1" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Gross weight of the associated entity. The total weight of the entity without reduction of packaging or container. I.e The Gross weight of a pallet of goods includes the weight of the pallet.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="TotalVolumeMeasure" id="oagis-id-9ea1adae54f44f459c40a88970b94f4d" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Quantifies the volume, or amount of space, that is occupied by an item, container or shipment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="IntraCompanyTransferIndicator" id="oagis-id-4ec70cbab0b34857a475145a6713bc3d" minOccurs="0"/>
					<xsd:element ref="ShippingRoute" id="oagis-id-290b4d3fcdf945018d777e7673d28196" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="ShipFromParty" id="oagis-id-c28b6753abf4487eac9ff418358ae1c0" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the party that the goods or services are being shipped or sent from.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShipToParty" id="oagis-id-b5b1b804be094f3d8e7dcc7adf4e8cce" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the ShipToParty for the document or the line. In a lot of cases, this is the same as the CustomerParty. In multi-organization purchasing, the CustomerParty may be different from the BuyerParty</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShipperParty" id="oagis-id-bacf038a48674b5d8d0a71ad54c18d99" minOccurs="0"/>
					<xsd:element ref="DeclarantParty" id="oagis-id-16bd758c7c314a329ae519c79cca5835" minOccurs="0"/>
					<xsd:element ref="TransportID" id="oagis-id-412c0d8cefa1403d85e1afd681ff3305" minOccurs="0"/>
					<xsd:element ref="EmergencyContact" id="oagis-id-e3ffc533ce104095805e6402babf5216" minOccurs="0"/>
					<xsd:element ref="Party" id="oagis-id-b5c4669911eb4c75949e13100804ea14" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="Event" id="oagis-id-95160b4fc1694d9caa6952afa02de7fc" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10"/>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
