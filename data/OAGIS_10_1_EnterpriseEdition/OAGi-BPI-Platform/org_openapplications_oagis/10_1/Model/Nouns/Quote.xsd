				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10">The item or product quoted along with quantity, price and other descriptive information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="typeCode" type="CodeType_1E7368" use="optional" id="oagis-id-80c2b7301f9245ca8bfa86a4bd79372e"/>
		<xsd:attribute name="actionCode" type="ActionCodeContentType" id="oagis-id-8bdf64cf6dc04272877e5135324ee60b"/>
	</xsd:complexType>
	<xsd:complexType name="QuoteHeaderBaseType" id="oagis-id-5b35f04f0a2041d8be8e49eca58fb2ce">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Defines the Quote Header structure.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ProcurementHeaderBaseType">
				<xsd:sequence>
					<xsd:element ref="RFQReference" id="oagis-id-bfc46565cc5243eb9baf3389f4f6f7a3" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is a reference to a RFQ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EffectiveTimePeriod" id="oagis-id-2d8dbd3cfa90460ca755f4466821145d" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the time period in which the associated component is effective.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="QualificationTerm" id="oagis-id-7e15c6faf6e841d5902ac414d66bb7e1" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is used to describe preferences that will be used to evaluate responses to an RFQ. For example, to identify a preference for sharing CAD data in VRML format the ID = VRML, Type = CAD and Value = 1 or Yes</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OrderCommission" id="oagis-id-f5a24c175ad94218aa19fbc6f5c11797" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Contains the relevant information about the salesperson and the associated commission information. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Message" id="oagis-id-8c777ee6695045e6a0bc459e5fb1586e" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
