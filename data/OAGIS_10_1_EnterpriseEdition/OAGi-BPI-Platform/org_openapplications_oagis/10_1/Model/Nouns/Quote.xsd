	<xsd:complexType name="QuoteLineBaseType" id="oagis-id-6b822f4f0ad041eb956fb33299f218b7">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Defines the Quote Line structure.</xsd:documentation>
