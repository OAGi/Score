						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is a reference to a RFQ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="EffectiveTimePeriod" id="oagis-id-9db543a71cac4a179da2d0bf795a29d8" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the time period in which the associated component is effective.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PromisedShipDateTime" id="oagis-id-1961db965340449fb7a7a6cabdd5fb42" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the date in which the associated object was promised to be shipped from the supplier.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PromisedDeliveryDateTime" id="oagis-id-2ccca10a91c7449eb78edf3ddc445d91" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the date in which the associated was promised to be delivered to the customer or consumer.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="DeliveryTimeTolerance" id="oagis-id-10ce38e9be744f2bbd73cee1b6d698b9" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicated the flexibility in the required delivery date that is allowed by the requestor</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="OrderCommission" id="oagis-id-2dbf7ca6eb15456690b1df997af7fa83" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Contains the relevant information about the salesperson and the associated commission information. </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Message" id="oagis-id-0c4650b380d1448f820ff7e98c4a1b04" minOccurs="0" maxOccurs="unbounded"/>
