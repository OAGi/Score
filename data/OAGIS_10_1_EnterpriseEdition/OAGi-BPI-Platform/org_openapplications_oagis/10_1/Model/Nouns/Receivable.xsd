			<xsd:element ref="ReceivableLine" id="oagis-id-24db37835a5d4c6a8d7a984790f55a26" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10">Line associated with the receivable document.  Can be an invoice line.  </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="typeCode" type="CodeType_1E7368" use="optional" id="oagis-id-e865ee7251bd4525bef9bfcdec2e9868"/>
