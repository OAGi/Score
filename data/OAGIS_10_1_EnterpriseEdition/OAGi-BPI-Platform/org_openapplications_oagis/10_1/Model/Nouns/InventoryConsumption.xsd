	<xsd:element name="InventoryConsumption" type="InventoryConsumptionType" id="oagis-id-8ac99dbec87b408091d50497bdb97477">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">This is used to share a consumer's usage information about an item or product with the supplier of the item or product</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="InventoryConsumptionHeader" type="InventoryConsumptionHeaderType" id="oagis-id-54ed5a848857415fa47196fe913d910b"/>
