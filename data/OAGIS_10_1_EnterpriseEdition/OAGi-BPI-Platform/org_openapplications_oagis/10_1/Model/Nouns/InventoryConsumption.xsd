		<xsd:sequence>
			<xsd:element ref="InventoryConsumptionHeader" id="oagis-id-66770bb51ec94d7abecd4ddd9b94ac0d" minOccurs="0"/>
			<xsd:element ref="InventoryConsumptionLine" id="oagis-id-151e721bf38a483fb7e46a5b625d7da0" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
