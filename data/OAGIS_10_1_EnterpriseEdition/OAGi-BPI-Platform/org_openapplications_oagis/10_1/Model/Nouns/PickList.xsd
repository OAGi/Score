	<xsd:complexType name="PickListLineDetailBaseType" id="oagis-id-4dfab1374c87405088e51f286a96cc9a">
		<xsd:complexContent>
			<xsd:extension base="LineBaseType">
				<xsd:sequence>
					<xsd:element ref="ShippingTrackingID" id="oagis-id-01e9e7d19f0142ba9826e907fbc2a8ab" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is a unique identifier for the purpose of tracking an individual package or shipment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="LoadingLocation" id="oagis-id-9595ca74c06d4118ba09319de4f812eb" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the location that shipped goods are to be loaded.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShipToLocation" id="oagis-id-dc73be0ec4fb434abd96ef3e5fdc4e0a" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the location in which the goods are to be shipped.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShippingMaterialID" id="oagis-id-a6e4f53e22074e24b590ec614ee9d4e2" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the materials used to ship.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShipmentDateTime" id="oagis-id-236cce5de90746d69f47328b5f342404" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the timestamp that indicates when the associated entity(ies) are shipped.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="LoadingDateTime" id="oagis-id-e8f81d5159054ade85ea5c8a8b2474b9" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the date and time a shipment loading process is complete.</xsd:documentation>
