				<xsd:annotation>
					<xsd:documentation source="http://www.openapplications.org/oagis/10">Information about a line item on a Picking List.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="typeCode" type="CodeType_1E7368" use="optional" id="oagis-id-ae84ca941a0d44d88f227dbff5a95614"/>
