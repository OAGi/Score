	<xsd:complexType name="PickListLineBaseType" id="oagis-id-f56b532171db4438866868a0920ba71a">
		<xsd:complexContent>
			<xsd:extension base="LineBaseType">
				<xsd:sequence>
					<xsd:element ref="ItemInstance" minOccurs="0" maxOccurs="unbounded" id="oagis-id-510739fdceb140808bf9a92b7a5cbd48"/>
					<xsd:element ref="Quantity" id="oagis-id-0db7e79d0ee24f6ca3c1b72ecba5785d" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:group ref="InventoryTransactionGroup" id="oagis-id-3873433dc08c4b4e91814c86854c1774"/>
					<xsd:element ref="OrderQuantity" id="oagis-id-a0396f8b2dfc415281412c86fdf5b088" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the quantity of a material or product ordered by the customer.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShippedQuantity" id="oagis-id-462edc7d706e412ab149559f8cb97f96" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the quantity that is  shipped.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShipUnitQuantity" id="oagis-id-ca689a3cc12145868c7eba61c2c82f04" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This contains the quantity of units or things shipped. This represents the container(s), not the product shipped.  An example of this is "4 truck loads" or "2 wooden crates"</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CarrierParty" id="oagis-id-ccd33be12d1c4bce8f11404acf9b71e2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Idenitfies the party that serves as the carrier of the goods or services for the transaction.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PickListLineDetail" id="oagis-id-be50bfc6b51343f08379f94d0675c09e" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10">Detail information about particular line items</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
