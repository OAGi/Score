	</xsd:complexType>
	<xsd:complexType name="PickListHeaderBaseType" id="oagis-id-706f598cb1e14dfc8bf95706419fcc96">
		<xsd:complexContent>
			<xsd:extension base="HeaderBaseType">
				<xsd:sequence>
					<xsd:element ref="DemandReference" id="oagis-id-5eb22dbafdd845abab0ad4e6ed6de910" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">This is the document that is being satisfied by the Pick List. This may be a sales order, inventory reservation etc</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="BillOfLadingReference" id="oagis-id-87311449986942f2afc1f5f63cd5ae36" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is a reference to the associated Bill of Lading document.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ShippingDockID" id="oagis-id-f86c96e61ba84c568305e7ad8735f45c" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Is the dock identifier that the associated entity or entities are to be shipped from or were shipped from.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="RouteID" id="oagis-id-d2f3a3a3ced941479900aaf536a787c9" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Identifies the physical route the associated entity is to take or has taken.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="WeightMeasure" id="oagis-id-7287192baf104395a7973c82cec3a13f" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the weight of the associated entity.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="VolumeMeasure" id="oagis-id-f2e50eac33b64fe6bde67fa8ca0deb5a" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Indicates the volume of the associated entity.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CarrierParty" id="oagis-id-7adb09fd5be245d695688b3625f1c719" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Idenitfies the party that serves as the carrier of the goods or services for the transaction.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="Party" id="oagis-id-c71efa7ed0a34deb8f27b20b2ef896ee" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A generic Party  for non-validated or application-validated parties. A work-around for situations where Schema-validation of Parties is not feasible or not desired.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
