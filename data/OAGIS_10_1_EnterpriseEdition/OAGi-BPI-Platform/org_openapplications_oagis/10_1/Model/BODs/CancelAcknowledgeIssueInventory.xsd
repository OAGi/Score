		<xsd:sequence>
			<xsd:element ref="CancelAcknowledge" id="oagis-id-177640fcd4ff4b0c871ff7407e5c69ea"/>
			<xsd:element ref="IssueInventory" maxOccurs="unbounded" id="oagis-id-8ea709d5f3674edbaf86cd03c2acfe1c"/>
		</xsd:sequence>
	</xsd:complexType>
