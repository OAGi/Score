--><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.openapplications.org/oagis/10" targetNamespace="http://www.openapplications.org/oagis/10" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:include schemaLocation="../Platform/2_3/Common/Components/Components.xsd"/>
	<xsd:element name="SequenceSchedule" type="SequenceScheduleType" id="oagis-id-4d1516bf03574833b2146b3ce11ce4df">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">A Sequence Schedule is used to indicate sequencial scheduling of ordered items in the manufacturing process. Commonly, the sequence schedule is generated by a work in process application and transmitted to an order or material planning application.
</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SequenceScheduleHeader" type="SequenceScheduleHeaderType" id="oagis-id-b38e2fcd656849b2be8202c829825a44">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Header level informatoin for the schedule.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
