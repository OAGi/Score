		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Sequence Schedule Date and Time information associated with the Schedule Line.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="SequenceScheduleLine" type="SequenceScheduleLineType" id="oagis-id-f2b6a9a686834fbca836505ba18b3b47">
		<xsd:annotation>
			<xsd:documentation source="http://www.openapplications.org/oagis/10">Line level informatoin for the schedule within a particular event.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
