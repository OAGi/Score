        <xsd:include schemaLocation="../Nouns/InventoryCount.xsd"/>
        <xsd:element name="ProcessInventoryCount" type="ProcessInventoryCountType" id="oagis-id-02835410244a4d1ea4573ab8ed5ceaa3">
                <xsd:annotation>
                        <xsd:documentation source="http://www.openapplications.org/oagis/10">The purpose of the ProcessInventoryCount is to transmit an inventory count to ERP from the actual physical inventory location. This count may be a cycle count or a physical count.  This BOD may also apply to planned or unplanned inventory counts.</xsd:documentation>
                </xsd:annotation>
        </xsd:element>
