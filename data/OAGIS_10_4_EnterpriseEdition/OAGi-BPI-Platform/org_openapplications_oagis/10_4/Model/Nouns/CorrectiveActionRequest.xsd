							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Requested date of completion of interim action.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ActualTimePeriod" minOccurs="0" id="oagis-id-7ca17c77cde74955a7ae0da653ca3af7">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Actual date of completion of interim action.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
