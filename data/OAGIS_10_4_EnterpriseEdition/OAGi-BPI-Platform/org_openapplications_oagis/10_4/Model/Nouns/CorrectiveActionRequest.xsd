						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A reference to the work order for the item instances related to this incident.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="PurchaseOrderReference" minOccurs="0" id="oagis-id-177147fdd873483380cf32b1ba802cf9">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">A reference to the purchase order for the item instances related to this incident.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="ProblemDescription" minOccurs="0" maxOccurs="unbounded" id="oagis-id-e40fa745cc8d486b937ac12bed04bc6b">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Description of the problem or problems  identified with the material in question.</xsd:documentation>
