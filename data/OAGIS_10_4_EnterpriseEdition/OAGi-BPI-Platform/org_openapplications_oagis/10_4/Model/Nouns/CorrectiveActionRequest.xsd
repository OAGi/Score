						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Lots, batches, dates, or other information that identify the collection of material associated with the nonconformance incident.
</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="TotalAffectedQuantity" minOccurs="0" id="oagis-id-0f9821d079fb4943bcaf4e309d6e2177">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The quantity of material affected by the problem over the period the history covers.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FailureSummaryResults" minOccurs="0" maxOccurs="unbounded" id="oagis-id-9477bcfd67324f01a80b470b34061830">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Test results related to the problem.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="FailureRatePercent" minOccurs="0" id="oagis-id-64da89c53551440192dc3acd99765be4">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">The percentage of material that was found to be nonconformant in this incident.</xsd:documentation>
