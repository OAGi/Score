						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Deadline for submitting the CorrectiveActionPlan.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element ref="CorrectiveActionDueDateTime" minOccurs="0" id="oagis-id-07b590eb5249431992f5b8e63af88d60">
						<xsd:annotation>
							<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Deadline for the correcting the problem.</xsd:documentation>
