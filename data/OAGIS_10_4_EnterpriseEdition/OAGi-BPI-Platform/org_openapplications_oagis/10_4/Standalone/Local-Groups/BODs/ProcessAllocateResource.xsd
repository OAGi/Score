			<xsd:documentation source="http://www.openapplications.org/oagis/10/platform/2">Provides the dimensions of the associated component.</xsd:documentation>
		</xsd:annotation>
</xsd:element>
					<xsd:element name="ExportControl" type="ExportControlType" id="oagis-id-56dd4ac3761c464b92b490c27e984499" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="HarmonizedTariff" type="HarmonizedTariffType" id="oagis-id-8a78ba572fca4330bdd530590ffa130b" minOccurs="0" maxOccurs="unbounded"/>
